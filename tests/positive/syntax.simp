group Node:
	visible word repr():
		back "<Node representation>"

group FuncDefNode inherits Node:
	visible word id
	visible collection[wiki[word, word]] params

	init(word id, collection[wiki[word, word]] params):
		self.id = id
		self.params = params

	visible word repr():
		back format("<function {}>", [id])

group ProgramNode inherits Node:
	visible collection[Node] global_nodes
	visible FuncDefNode home_node

	visible word repr():
		back "<program>"

home():
	ProgramNode program = new ProgramNode()
	program.home_node(new FuncDefNode("home", [{"string":"arg"}]))